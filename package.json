{
  "name": "contact-manager",
  "version": "1.0.0",
  "description": "App to manage contacts",
  "engines": {
    "yarn": "1.16.0",
    "node": "12.14.0"
  },
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && yarn run start",
    "clientinstall": "cd client && yarn",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "heroku-postbuild": "YARN_PRODUCTION=false cd client && yarn install && yarn run build"
  },
  "author": "Paul Miller",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.2.5",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.12"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
